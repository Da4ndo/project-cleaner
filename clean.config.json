{
  "dir": "/projects",
  "target": {
    "files_patterns": ["\\.pyc$", "\\.o$", "\\.a$"],
    "dirs_patterns": ["target$", "node_modules$", "build$", "dist$"]
  },
  "exclude": {
    "files_patterns": ["README\\.md$", "LICENSE$"],
    "dirs_patterns": ["project-cleaner$", "\\.git$", "\\.vscode$", "test$"]
  },

  "backup": {
    "enabled": true,
    "dir": "~/.backup",
    "versioning": false
  }
}